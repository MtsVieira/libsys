<%-include('../partials/header.html')%>
    <link rel="stylesheet" href="/../../css/admin.css">
    <titulo>Deletar Usuário</titulo>
    </head>

    <body>

        <%-include('partials/navbar.html')%>
            <div class="container-fluid">
                <div class="row">
                    <%-include('partials/sidebar.html')%>
                        <div class="col-md-10 content">
                            <div class="row">
                                <div class="col-md-11 modify-usuario">
                                    <p class="display-4">Deletar Usuário</p>
                                    <form class="" method="post">
                                        <input type="text" name="user_id" value="<%=res.usuario_id%>" hidden>
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Nome</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" name="name" value="<%=res.nome%>" readonly>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Email</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" name="email" value="<%=res.email%>" readonly>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Telefone</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" name="phone" value="<%=res.telefone%>" readonly>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Curso</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" name="curso" value="<%=res.curso%>" readonly>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Gênero</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" name="gender" value="<%=res.genero%>" readonly>
                                            </div>
                                        </div>
                                        <hr>
                                        <div>
                                            <button type="submit" class="btn btn-primary">Deletar</button>
                                            <br>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                </div>
            </div>

            <%-include('../partials/footer.html')%>
